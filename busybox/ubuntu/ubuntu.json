{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "name": "ubuntu-echo",
    "namespace": "default"
  },
  "spec": {
    "containers": [
      {
        "image": "registry.docker.company.com:5000/ubuntu-echo",
        "imagePullPolicy": "IfNotPresent",
        "name": "ubuntu-echo"
      }
    ],
    "restartPolicy": "Always"
  }
}